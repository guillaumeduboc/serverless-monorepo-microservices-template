{
  "root": "backend/core",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [
        "backend/core/.serverless"
      ],
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        },
        {
          "target": "package",
          "projects": "dependencies"
        }
      ],
      "options": {
        "cwd": "backend/core",
        "commands": [
          {
            "command": "serverless deploy"
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "dependsOn": [
        {
          "target": "package",
          "projects": "dependencies"
        }
      ],
      "options": {
        "cwd": "backend/core",
        "commands": [
          {
            "command": "yarn test:all"
          }
        ]
      }
    },
    "tags": [],
    "implicitDependencies": []
  }
}