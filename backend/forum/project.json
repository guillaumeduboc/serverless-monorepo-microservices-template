{
  "root": "backend/forum",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [
        "backend/forum/.serverless"
      ],
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        },
        {
          "target": "package",
          "projects": "dependencies"
        }
      ],
      "options": {
        "cwd": "backend/forum",
        "commands": [
          {
            "command": "serverless deploy"
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "dependsOn": [
        {
          "target": "package",
          "projects": "dependencies"
        }
      ],
      "options": {
        "cwd": "backend/forum",
        "commands": [
          {
            "command": "yarn test:all"
          }
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "backend-core"
  ]
}