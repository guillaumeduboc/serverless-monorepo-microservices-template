{
  "root": "frontend/app",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [
        "frontend/app/dist"
      ],
      "options": {
        "cwd": "frontend/app",
        "commands": [
          {
            "command": "yarn build"
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "dependsOn": [
        {
          "target": "package",
          "projects": "dependencies"
        }
      ],
      "options": {
        "cwd": "frontend/app",
        "commands": [
          {
            "command": "yarn test:all"
          }
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": []
}